    <script>
        var iframeDiv = document.querySelector("#iframe_div");
        var input = document.querySelector("#input");
        var jiekou = 'https://17kyun.com/api.php?url=', url = null;
        function jkChanged(v){
            jiekou = v.value;
            play();
        }
        function play() {
            _hmt.push(['_trackEvent', 'video', 'click', 'play']);
            url = input.value.trim();
            if (!url) {
                input.focus();
                return;
            }
            if (jiekou && url) {
                var link = jiekou + url;
                iframeDiv.innerHTML = '<iframe scrolling="auto" onload="changeFrameHeight()" allowfullscreen="true" frameborder="0" src="' + link + '">';
            }
        }
        function playToMobile() {
            var dateStr = Math.random();
            document.getElementById("qrcode").src = '/api/qr_code?url=' + input.value.trim() + '&time=' + dateStr;
            document.querySelector('#alert').style.display = 'block';
        }

        var _hmt = _hmt || [];
        (function() {
          var hm = document.createElement("script");
